    "marked": "2.0.3",
    "vue": "2.6.14",
    "vue-router": "3.5.1",
    "vue-template-compiler": "2.6.14",
    "vuex": "3.6.2",
    "vuex-persistedstate": "3.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.14.3",
