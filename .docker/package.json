{
  "name": "styleguide_devtools",
  "version": "0.9.2",
  "description": "Oslo Style Guide devtools",
  "main": "index.js",
  "scripts": {
    "hmr": "webpack-dev-server --hot --config config/app/webpack.dev.js",
    "dev": "rm -rf dist/* && npm run build-structure \"false\" && php src/utils/twigCompiler.php && webpack --config config/modules/webpack.dev.js --mode development && npm run build-structure \"true\"",
    "dev-modules": "webpack --config config/modules/webpack.dev.js --mode development",
    "build-structure": "./node_modules/.bin/babel-node src/utils/buildAtomicStructure.js",
    "build-modules": "webpack --config config/modules/webpack.production.js --mode production",
    "build-viewer": "webpack --config config/app/webpack.production.js --mode production",
    "build-production": "rm -rf dist/* && npm run build-structure \"false\" && php src/utils/twigCompiler.php && npm run build-modules && npm run build-structure \"true\" && npm run build-viewer",
    "mkpat": "babel-node --presets env src/utils/newPattern.js",
    "watch": "onchange --kill --initial --no-exclude \"node_modules/styleguide/src/**/*\" -- ./node_modules/.bin/babel-node eventListener.js {{event}} {{changed}}",
    "hmr-watch": "npm run dev && concurrently \"npm run hmr\" \"npm run watch\" --kill-others --kill-others-on-fail"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/oslokommune/styleguide_devtools.git"
  },
  "author": "Lars Boldt",
  "license": "MIT",
  "homepage": "https://github.com/oslokommune/styleguide_devtools#readme",
  "peerDependencies": {
    "styleguide": "github:oslokommune/styleguide#semver:0.35.4"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "5.8.1",
    "axe-core": "3.2.2",
    "bulma": "0.7.4",
    "dotenv-webpack": "1.7.0",
    "font-face-mixin": "1.1.0",
    "https-proxy-agent": "^3.0.1",
    "js-beautify": "1.9.1",
    "marked": "^0.7.0",
    "minireset.css": "0.0.3",
    "serialize-javascript": ">=2.1.1",
    "vue": "2.6.10",
    "vue-color": "2.7.0",
    "vue-highlightjs": "1.3.3",
    "vue-router": "3.0.6",
    "vue-template-compiler": "2.6.10",
    "vuex": "3.1.0",
    "vuex-persistedstate": "2.5.4"
  },
  "devDependencies": {
    "@babel/cli": "7.4.3",
    "@babel/core": "7.4.3",
    "@babel/node": "7.2.2",
    "@babel/preset-env": "7.4.3",
    "babel-loader": "8.0.5",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "concurrently": "4.1.0",
    "copy-webpack-plugin": "5.0.3",
    "css-loader": "2.1.1",
    "dotenv": "7.0.0",
    "eslint": "5.16.0",
    "eslint-loader": "2.1.2",
    "eslint-plugin-vue": "5.2.2",
    "file-loader": "3.0.1",
    "fs": "0.0.1-security",
    "github-markdown-css": "3.0.1",
    "glob": "7.1.3",
    "hard-source-webpack-plugin": "0.13.1",
    "html-webpack-plugin": "3.2.0",
    "inquirer": "6.3.1",
    "lodash": "^4.17.15",
    "mini-css-extract-plugin": "0.6.0",
    "node-sass": "4.11.0",
    "npm-install-peers": "^1.2.1",
    "onchange": "5.2.0",
    "resolve-url-loader": "3.1.0",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "vue-loader": "15.7.0",
    "webpack": "4.30.0",
    "webpack-cli": "3.3.1",
    "webpack-dev-server": "3.3.1",
    "webpack-merge": "4.2.1"
  }
}
