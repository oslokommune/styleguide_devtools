{
  "name": "styleguide_devtools",
  "version": "1.0.0",
  "description": "Oslo Style Guide devtools",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "hmr": "webpack serve --hot --config config/webpack.dev.cjs",
    "dev": "npm run build-js && npm run build-structure \"true\"",
    "build-js": "./node_modules/.bin/rollup -c",
    "build-structure": "./node_modules/.bin/babel-node src/utils/buildComponentStructure.js",
    "build-viewer": "webpack --config config/webpack.production.cjs --mode production",
    "watch": "onchange --kill --initial --no-exclude \"node_modules/styleguide/src/**/*\" -- ./node_modules/.bin/babel-node eventListener.js {{event}} {{changed}}",
    "hmr-watch": "npm run dev && concurrently \"npm run hmr\" \"npm run watch\" --kill-others --kill-others-on-fail"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/oslokommune/styleguide_devtools.git"
  },
  "author": "Lars Boldt",
  "license": "MIT",
  "homepage": "https://github.com/oslokommune/styleguide_devtools#readme",
  "dependencies": {
    "@fortawesome/fontawesome-free": "5.15.3",
    "dotenv-webpack": "7.0.2",
    "marked": "^2.0.3",
    "rollup-plugin-vue": "^5.1.9",
    "vue": "2.6.12",
    "vue-router": "3.5.1",
    "vue-template-compiler": "2.6.12",
    "vuex": "3.6.2",
    "vuex-persistedstate": "3.2.0"
  },
  "devDependencies": {
    "@babel/cli": "7.13.10",
    "@babel/core": "7.13.10",
    "@babel/node": "7.13.12",
    "@babel/plugin-transform-for-of": "7.13.0",
    "@babel/preset-env": "7.13.12",
    "@rollup/plugin-babel": "^5.3.0",
    "autoprefixer": "10.2.5",
    "babel-loader": "8.2.2",
    "case-sensitive-paths-webpack-plugin": "2.4.0",
    "concurrently": "6.0.0",
    "copy-webpack-plugin": "^8.1.0",
    "css-loader": "5.2.0",
    "dotenv": "8.2.0",
    "eslint": "7.22.0",
    "eslint-plugin-vue": "7.8.0",
    "eslint-webpack-plugin": "2.5.3",
    "file-loader": "6.2.0",
    "glob": "7.1.6",
    "html-webpack-plugin": "5.3.1",
    "mini-css-extract-plugin": "1.3.9",
    "onchange": "7.1.0",
    "postcss": "^8.2.15",
    "postcss-loader": "5.3.0",
    "resolve-url-loader": "^4.0",
    "rollup": "^2.48.0",
    "sass": "^1.32.8",
    "sass-loader": "11.0.1",
    "vue-loader": "15.9.7",
    "webpack": "5.28.0",
    "webpack-cli": "4.5.0",
    "webpack-dev-server": "3.11.2",
    "webpack-merge": "5.7.3"
  }
}
